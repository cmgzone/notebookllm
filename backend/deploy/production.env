# Production Environment Configuration
NODE_ENV=production
PORT=3000

# Database
DATABASE_URL=your_production_database_url

# Authentication
JWT_SECRET=your_production_jwt_secret

# Redis
REDIS_URL=redis://redis:6379

# Payment
STRIPE_SECRET_KEY=your_production_stripe_key
STRIPE_WEBHOOK_SECRET=your_production_stripe_webhook_secret

# AI Services
OPENAI_API_KEY=your_production_openai_key
GEMINI_API_KEY=your_production_gemini_key
ELEVENLABS_API_KEY=your_production_elevenlabs_key
DEEPGRAM_API_KEY=your_production_deepgram_key
MURF_API_KEY=your_production_murf_key
SERPER_API_KEY=your_production_serper_key

# GitHub Integration
GITHUB_CLIENT_ID=your_production_github_client_id
GITHUB_CLIENT_SECRET=your_production_github_client_secret

# Google Calendar OAuth
GOOGLE_CALENDAR_CLIENT_ID=your_production_google_calendar_client_id
GOOGLE_CALENDAR_CLIENT_SECRET=your_production_google_calendar_client_secret
GOOGLE_CALENDAR_REDIRECT_URI=https://backend.taskiumnetwork.com/api/gitu/calendar/callback

# CORS
CORS_ORIGIN=https://yourapp.com

# Logging
LOG_LEVEL=info

# Performance
MAX_OLD_SPACE_SIZE=6144
