# Staging Environment Configuration
NODE_ENV=staging
PORT=3000

# Database
DATABASE_URL=your_staging_database_url

# Authentication
JWT_SECRET=your_staging_jwt_secret

# Redis
REDIS_URL=redis://redis:6379

# Payment
STRIPE_SECRET_KEY=your_staging_stripe_key
STRIPE_WEBHOOK_SECRET=your_staging_stripe_webhook_secret

# AI Services
OPENAI_API_KEY=your_staging_openai_key
GEMINI_API_KEY=your_staging_gemini_key
ELEVENLABS_API_KEY=your_staging_elevenlabs_key
DEEPGRAM_API_KEY=your_staging_deepgram_key
SERPER_API_KEY=your_staging_serper_key

# GitHub Integration
GITHUB_CLIENT_ID=your_staging_github_client_id
GITHUB_CLIENT_SECRET=your_staging_github_client_secret

# Google Calendar OAuth
GOOGLE_CALENDAR_CLIENT_ID=your_staging_google_calendar_client_id
GOOGLE_CALENDAR_CLIENT_SECRET=your_staging_google_calendar_client_secret
GOOGLE_CALENDAR_REDIRECT_URI=https://backend.taskiumnetwork.com/api/gitu/calendar/callback

# CORS
CORS_ORIGIN=https://staging.yourapp.com

# Logging
LOG_LEVEL=debug
