{
  "name": "notebook-llm-backend",
  "version": "2.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node --max-old-space-size=6144 dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:watch": "tsx watch src/index.ts",
    "lint": "tsc -p tsconfig.json --noEmit",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
    "test:pbt": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --testMatch \"**/*.pbt.test.ts\"",
    "test:qr-mobile": "tsx src/scripts/test-qr-auth-mobile.ts",
    "test:qr-mobile:full": "tsx src/scripts/test-qr-auth-mobile.ts --with-expiry",
    "migrate:agent-communication": "tsx src/scripts/run-agent-communication-migration.ts",
    "migrate:gitu-core": "tsx src/scripts/run-gitu-core-migration.ts",
    "migrate:google-calendar": "tsx src/scripts/run-google-calendar-migration.ts",
    "migrate:mcp-user-limits": "tsx src/scripts/run-mcp-user-limits-migration.ts",
    "verify:swarm-schema": "tsx src/scripts/verify-swarm-schema.ts",
    "gitu-cli": "tsx src/scripts/gitu-cli.ts",
    "gitu": "tsx src/scripts/gitu-cli.ts"
  },
  "keywords": [
    "notebook",
    "llm",
    "ai",
    "backend",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "description": "Complete backend API for Notebook LLM Flutter app with all features",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@modelcontextprotocol/sdk": "^1.0.1",
    "@octokit/rest": "^21.1.1",
    "@types/node-telegram-bot-api": "^0.64.13",
    "@whiskeysockets/baileys": "^7.0.0-rc.9",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.3.0",
    "cheerio": "^1.1.2",
    "chrono-node": "^2.7.8",
    "commander": "^12.1.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "node-telegram-bot-api": "^0.67.0",
    "ora": "^8.1.1",
    "pdf-parse": "^1.1.4",
    "pg": "^8.14.0",
    "playwright": "^1.58.1",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "redis": "^5.10.0",
    "stripe": "^20.1.0",
    "uuid": "^11.0.5",
    "ws": "^8.18.0",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "fast-check": "^3.22.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.2.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
