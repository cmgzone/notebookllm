{
  "name": "notebook-llm-backend",
  "version": "2.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:watch": "tsx watch src/index.ts",
    "lint": "eslint src/**/*.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
    "test:pbt": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand --testPathPattern=pbt",
    "migrate:agent-communication": "tsx src/scripts/run-agent-communication-migration.ts"
  },
  "keywords": [
    "notebook",
    "llm",
    "ai",
    "backend",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "description": "Complete backend API for Notebook LLM Flutter app with all features",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@octokit/rest": "^21.1.1",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.14.0",
    "stripe": "^20.1.0",
    "uuid": "^11.0.5",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "fast-check": "^3.22.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
